{
  "version": 2,
  "builds": [
    {
      "src": "StellerProject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_staticfiles.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles",
        "buildCommand": "chmod +x build_staticfiles.sh && ./build_staticfiles.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)",
      "dest": "StellerProject/wsgi.py"
    }
  ]
}
